{
  "expo": {
    "name": "MBC",
    "slug": "library-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/mbc-logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/mbc-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/mbc-logo.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      },

      "package": "com.philangcamu.libraryapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "5b726692-9fb6-438c-a1d1-f368d2a4d629"
      }
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/mbc-logo.png",
          "color": "#ffffff"
        }
      ]
    ]
  }
}
